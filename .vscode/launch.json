{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Pytest",
      "type": "python",
      "request": "launch",
      "module": "pytest",
      "args": [
        "--cov=pyshared",
        "--cov-report=term-missing",
        "--cov-report=html",
        "-vv",
        "tests.py"
      ],
      "console": "integratedTerminal"
    }
  ]
}
